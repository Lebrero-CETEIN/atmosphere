= form_for [:admin, @appliance_set] do |f|
  - if @appliance_set.errors.any?
    #error_explanation
      %h2= "#{pluralize(@appliance_set.errors.count, "error")} prohibited this appliance set from being saved:"
      %ul
        - @appliance_set.errors.full_messages.each do |msg|
          %li= msg

  =# f.select :appliance_set_type, ApplianceSet.appliance_set_type.options

  .actions
    = f.submit 'Save'
