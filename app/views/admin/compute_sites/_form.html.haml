= simple_form_for([:admin, @compute_site]) do |f|
  = f.error_notification

  .inputs
    = f.input :site_id, label: 'Site id'
    = f.input :name
    = f.input :location
    = f.input :site_type
    = f.input :technology
    = f.input :http_proxy_url
    = f.input :https_proxy_url
    = f.input :config, as: :text

  .actions
    = f.button :submit
