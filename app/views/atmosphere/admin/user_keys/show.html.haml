%p#notice= notice

%p
  %table.table.table-hover.table-condensed
    %tr
      %td
        %b
          = t('user_keys.name')
      %td
        = @user_key.name
    %tr
      %td
        %b
          = t('user_keys.fingerprint')
      %td{ style: "font-family: monospace;" }
        = @user_key.fingerprint

%p
  .btn-group
    = link_to admin_user_keys_path,
      class: 'btn btn-default', title: t('back') do
      =icon 'reply'
      = t('back')
